{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "umd",
    "lib": [ "ESNext" ],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "declaration": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
